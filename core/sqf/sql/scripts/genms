echo "#setting MY_UDR_ROOT"
if [ -d "/home/udr" ]; then
  w=`whoami`
  dircreate=/home/udr/$w
  if [ ! -d $dircreate ]; then
    mkdir --mode=775 $dircreate 2>/dev/null
  fi
  echo "MY_UDR_ROOT=$dircreate"
else
  if [ ! -d "$MY_SQROOT/udr" ]; then
    mkdir $MY_SQROOT/udr 2>/dev/null
  fi
  echo "MY_UDR_ROOT=\$MY_SQROOT/udr"
fi

echo ""
