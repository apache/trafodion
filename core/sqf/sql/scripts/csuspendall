function LogIt {
    echo  "`date`: $1" >> $TRAF_LOG/startup.log
}

lv_prog_name="csuspendall"

echo "Going to execute $lv_prog_name"
cd $TRAF_HOME/sql/scripts

setup_sqpdsh
eval '$SQPDSHA "$TRAF_HOME/sql/scripts/psuspendall"'

LogIt "$lv_prog_name"


