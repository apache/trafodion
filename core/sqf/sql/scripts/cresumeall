function LogIt {
    echo  "`date`: $1" >> $MY_SQROOT/logs/startup.log
}

lv_prog_name="cresumeall"

echo "Going to execute $lv_prog_name"
setup_sqpdsh
eval '$SQPDSHA "$MY_SQROOT/sql/scripts/presumeall"'

LogIt "$lv_prog_name"

