export TEST_STOP=test.status
export SQ_MON_ENV_CHECK_DISABLE=1
#export MON_DTM_PRIMITIVE_DISABLE=1
cluster=
if [ "$1" = "-cluster" ]; then
	cluster=-cluster
	host1=`gohost 1`
	host2=`gohost 2`
	host3=`gohost 3`
	ssh $host1 "echo $host1"
	ssh $host2 "echo $host2"
	ssh $host3 "echo $host3"
	shift
fi
verbose=
if [ "$1" = "-verbose" ]; then
	verbose=-verbose
	shift
fi
gostart
go2 $cluster $verbose
go3 $cluster $verbose
go4 $cluster $verbose
