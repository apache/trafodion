#MS_TRACE_TIMER=1
#MS_TRACE_TIMERMAP=1
#MS_TRACE_TRANS=1
#MS_TRACE_VERBOSE=1
#MS_TRACE_WAIT=1

# fs flags
# - if there's an FS error, this causes an assert
FS_ASSERT_ERROR=1
# - trace flags
FS_TRACE_ENABLE=0
#FS_TRACE=1
#FS_TRACE_API=1
#FS_TRACE_DATA=1
#FS_TRACE_DATA_MAX=512
#FS_TRACE_DETAIL=1
#FS_TRACE_ERRORS=1
#FS_TRACE_FC=1
FS_TRACE_FILE=sbt
#FS_TRACE_FILE_DIR=<directory>
FS_TRACE_FILE_UNIQUE=1
#FS_TRACE_FLUSH=1
FS_TRACE_MON=1
#FS_TRACE_MT=1
FS_TRACE_PARAMS=1
#FS_TRACE_VERBOSE=1

# capacity limits
#FS_MAX_CAP_FDS=20
#MS_MAX_CAP_MDS=25
#MS_MAX_CAP_ODS=3
#MS_MAX_CAP_PHANDLES=1

# Turn on to enable sock transport
#SQ_TRANS_SOCK=1

# profiling flags
# enable will create profiles named z<cmdline>.<name>.<pid>.prof.
#SQ_GP_ENABLE=1
# sig will flush profiler files if SIGUSR2 received
#SQ_GP_SIG=1

# debug flags
# - if client and file 'c' does not exit go into debug
DEBUGC=1
# - if server and file 's' does not exit go into debug
DEBUGS=1

# hook - if set allows debug hook
HOOK_ENABLE=0

# test flags
# - go* scripts check to see if this file exists
#       as it's created at the end of a test.
TEST_STOP=test.status

SQ_EVLOG_NONE=1
