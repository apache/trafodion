#MS_TRACE_FILE_DIR=<directory>
#MS_TRACE_FILE_FB=65536
#MS_TRACE_FILE_INMEM=65536
#MS_TRACE_FILE_MAXSIZE=3000000000
#MS_TRACE_FILE_NOLOCK=1
#MS_TRACE_FILE_SIGNAL=1
MS_TRACE_FILE_UNIQUE=1
#MS_TRACE_IC=1
#MS_TRACE_LOCIO=1
#MS_TRACE_MD=1
MS_TRACE_MON=1
#MS_TRACE_NAME=1
MS_TRACE_PARAMS=1
