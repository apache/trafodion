# - trace flags
MS_TRACE_ENABLE=0
#MS_TRACE=1
#MS_TRACE_ALLOC=1
#MS_TRACE_DATA=1
#MS_TRACE_DATA_MAX=512
#MS_TRACE_DETAIL=1
#MS_TRACE_DIALECT=1
#MS_TRACE_ENVIRON=1
#MS_TRACE_ERRORS=1
#MS_TRACE_EVENTS=1
#MS_TRACE_EVLOG=1
MS_TRACE_FILE=sbt
