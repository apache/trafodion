set pattern $$JARF$$ t100.jar;
log;

log LOG100;

?section set_up

create library T100 FILE $$QUOTE$$ $$REGRRUNDIR$$/$$JARF$$ $$QUOTE$$;

create procedure T100_io_nn(IN IN1 numeric(9,3), OUT OUT2 numeric(9,3))
external name 't100.T100_io_nn' language java no sql
library T100;

