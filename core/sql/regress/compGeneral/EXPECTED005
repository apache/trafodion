+>     t005t03                                                        
+>   FULL OUTER JOIN                                                   
+>        (select t005t04.seqno  as b                                            
+>              from t005t04 INNER JOIN t005t05                         
+>              ON t005t04.seqno = t005t05.seqno                      
+>         ) as t2                                                      
+>on t005t03.seqno = t2.b 
+>order by 1,2;

