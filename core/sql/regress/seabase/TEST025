                        GENERATED BY DEFAULT AS IDENTITY
                         (START WITH 60  
                          INCREMENT BY 1
                          MAXVALUE 100 MAXVALUE 30
                          MINVALUE 50
                           NO CYCLE)
                           NOT NULL NOT DROPPABLE,
                      b     INT UNSIGNED NOT NULL,
                      c     INT UNSIGNED,
