                          MINVALUE 50
                           NO CYCLE NO CYCLE)
                           NOT NULL NOT DROPPABLE,
                      b     INT UNSIGNED NOT NULL,
                      c     INT UNSIGNED,
                      primary key(a) )
 --HASH2 PARTITION BY(a)
                    ;-- (ADD LOCATION $$partition$$);
                    
