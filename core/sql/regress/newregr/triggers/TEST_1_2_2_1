-- forbidden semantics
-- Dropping a non existing trigger a


obey TEST_1_2_2_1(clean_up);
obey TEST_1_2_2_1(set_up);
log   LOG_1_2_2_1 clear;
obey TEST_1_2_2_1(tests);
LOG;
obey TEST_1_2_2_1(clean_up);
exit;

?section clean_up
set schema CAT1.SCHM;
 
obey clearTables;

?section set_up
?section tests
SET SCHEMA cat1.schm;
------------------------------------------------------------------
-- 	TEST CASE
------------------------------------------------------------------
-- the follwong command should fail
DROP TRIGGER djfyg987uegyf;

