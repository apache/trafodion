>>?section setIdxLevel
>>------------------------------------------------------------
>>--  Clear out TABID table and insert table ids for tables --
>>-- and indexes in the fake database.                      --
>>------------------------------------------------------------
>>execute clearTABID;

--- 1 row(s) deleted.
>>
>>
>>insert into TABID
+>  select object_uid
+>    from  HP_DEFINITION_SCHEMA.OBJECTS, SCHID
+>    where     object_type in ('BT', 'IX')
+>          and schema_uid  = sch_id
+>          and object_name in ('CUSTOMER',
+>                              'LINEITEM',
+>                              'ORDERS',
+>                              'PARTSUPP',
+>                              'PART',
+>                              'SUPPLIER',
+>			      'NATION',
+>			      'REGION',
+>			      'CUIDX1',
+>			      'CUIDX2',
+>			      'LIIDX1',
+>			      'LIIDX2',
+>			      'LIIDX3',
+>			      'LIIDX4',
+>			      'LIIDX5',
+>			      'LIIDX6',
+>			      'ORDIDX1',
+>			      'ORDIDX2',
+>			      'ORDIDX3',
+>			      'PAIDX1',
+>			      'PSIDX1',
+>			      'PSIDX2',
+>			      'SUIDX1',
+>			      'SUIDX2'
+>                             );

--- 24 row(s) inserted.
>>
>>
>>#ifMP
>>insert into TABID
+>  select T.createtime
+>  from  TABLES T
+>  where
+>    T.tabletype = 'TA'
+>    and
+>    T.tablename in
+>    ( -- The set we're interested in.
+>      select U1.usingobjname from USAGES U1
+>      where
+>        ( -- Match object names in the set without catalog names.
+>          substring(
+>            U1.usingobjname from (char_length(trim(U1.usingcatalogname))+2)
+>          )
+>          in
+>          ('CUSTOMER',
+>           'LINEITEM',
+>           'ORDERS',
+>           'PARTSUPP',
+>           'PART',
+>           'SUPPLIER',
+>	   'NATION',
+>	   'REGION',
+>	   'CUIDX1',
+>	   'CUIDX2',
+>	   'LIIDX1',
+>	   'LIIDX2',
+>	   'LIIDX3',
+>	   'LIIDX4',
+>	   'LIIDX5',
+>	   'LIIDX6',
+>	   'ORDIDX1',
+>	   'ORDIDX2',
+>	   'ORDIDX3',
+>	   'PAIDX1',
+>	   'PSIDX1',
+>	   'PSIDX2',
+>	   'SUIDX1',
+>	   'SUIDX2'
+>           )
+>        )
+>    )
+>  UNION
+>  select I.createtime
+>  from INDEXES I
+>  where
+>    I.tablename <> I.indexname
+>    and
+>    I.indexname in
+>    ( -- The set we're interested in.
+>      select U1.usingobjname from USAGES U1
+>      where
+>        ( -- Match object names in the set without catalog names.
+>          substring(
+>            U1.usingobjname from (char_length(trim(U1.usingcatalogname))+2)
+>          )
+>          in
+>          ('CUSTOMER',
+>           'LINEITEM',
+>           'ORDERS',
+>           'PARTSUPP',
+>           'PART',
+>           'SUPPLIER',
+>	   'NATION',
+>	   'REGION',
+>	   'CUIDX1',
+>	   'CUIDX2',
+>	   'LIIDX1',
+>	   'LIIDX2',
+>	   'LIIDX3',
+>	   'LIIDX4',
+>	   'LIIDX5',
+>	   'LIIDX6',
+>	   'ORDIDX1',
+>	   'ORDIDX2',
+>	   'ORDIDX3',
+>	   'PAIDX1',
+>	   'PSIDX1',
+>	   'PSIDX2',
+>	   'SUIDX1',
+>	   'SUIDX2'
+>           )
+>        )
+>    )
+>;
>>#ifMP
>>
>>------------------------------------------
>>--  Update the index level to 3 for all --
>>-- base tables and secondaryindexes.    --
>>------------------------------------------
>>
>>update HP_DEFINITION_SCHEMA.partitions
+>  set index_level=3
+>  where object_uid in (select tab_id from TABID);

--- 178 row(s) updated.
>>
>>
>>
>>exit;

