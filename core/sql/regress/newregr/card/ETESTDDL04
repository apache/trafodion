>>                     -- Interval count
>>set param ?b 1000000;
>>               -- Total row count
>>set param ?c 977910;
>>               -- Total unique entry count (UEC)
>>set param ?d  '3006-01-30 00:58:59.0';
>> -- Histogram timestamp
>>set param ?e  '(''Customer  bold dependencies ca'')';
>>               -- Low Value
>>set param ?f  '(''waters whithout the furiously'')';
>>          -- High Value
>>set param ?g  '1';
>>          -- number in group
>>execute updateHistogram;

--- 1 row(s) updated.
