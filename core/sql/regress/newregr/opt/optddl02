?section cx1
------------------------------------------------
-- Create nation key index on customer table. --
------------------------------------------------
#ifMX
Create unique index cx1 on customer
(
     c_nationkey 
   , c_custkey 
)
location $$partition2$$
partition( 
add first key               3     location    $$partition2$$ ,
add first key               5     location    $$partition1$$,
add first key               7     location    $$partition1$$,
add first key               9     location    $$partition2$$ ,
add first key              11     location    $$partition2$$ ,
add first key              13     location    $$partition1$$,
add first key              15     location    $$partition1$$,
add first key              17     location    $$partition2$$ ,
add first key              19     location    $$partition2$$ ,
add first key              21     location    $$partition1$$,
add first key              23     location    $$partition1$$ 
)
;
-- BUFFERED is not supported
-- attribute buffered; 
#ifMX

#ifMP
create unique index $$NSK_VOL$$.$$NSK_SUBVOL$$.CX1 on $$NSK_VOL$$.$$NSK_SUBVOL$$.CUSTOMER
(
     c_nationkey 
   , c_custkey 
)
catalog $$NSK_VOL$$.$$NSK_SUBVOL$$
partition( 
  $VDP002.$$NSK_SUBVOL$$.CX1
  CATALOG $$NSK_VOL$$.$$NSK_SUBVOL$$
  PHYSVOL $DATA06
