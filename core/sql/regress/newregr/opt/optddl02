?section ox1
-----------------------------------------
--  Create clerk index on order table. --
-----------------------------------------
#ifMX
Create index ox1 on orders
(
     o_clerk
   , o_orderdate 
)

location $$partition2$$
partition( 
add first key     'Clerk#000006250'   location    $$partition2$$,
add first key     'Clerk#000012500'   location    $$partition1$$,
add first key     'Clerk#000018750'   location    $$partition1$$,
add first key     'Clerk#000025000'   location    $$partition2$$,
add first key     'Clerk#000031250'   location    $$partition2$$,
add first key     'Clerk#000037500'   location    $$partition1$$,
add first key     'Clerk#000043750'   location    $$partition1$$,
add first key     'Clerk#000050000'   location    $$partition2$$,
add first key     'Clerk#000056250'   location    $$partition2$$,
add first key     'Clerk#000062500'   location    $$partition1$$,
add first key     'Clerk#000068750'   location    $$partition1$$,
add first key     'Clerk#000075000'   location    $$partition2$$,
add first key     'Clerk#000081250'   location    $$partition2$$,
add first key     'Clerk#000087500'   location    $$partition1$$,
add first key     'Clerk#000093750'   location    $$partition1$$ 
)
;
-- BUFFERED is not supported
-- attribute buffered; 
#ifMX

