-- Update timestamp for ORDERS table to reflect                       --
-- recently modified histogram statistics                             --
------------------------------------------------------------------------
#ifMX
execute updateTimeStamp;
#ifMX

------------------------------------------------------------------------
-- Display histogram information to verify that it was faked properly --
------------------------------------------------------------------------
execute displayHist;
execute displayIntervals;

